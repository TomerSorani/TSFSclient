            <Tab fx:id="ManagerTab" text="Manager">
              <content>
                  <GridPane>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <fx:include fx:id="ManagerControllerTabContent" source="mannagerController.fxml" />
                     </children>
                  </GridPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </content>
</ScrollPane>
